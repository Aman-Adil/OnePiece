choice=int(input("whts ur number of choice"))
def function(choice):

   for num in range (0, choice):
      if(num%3==0 and num%5==0):
        print("fizzbuzz")
      elif (num % 3 == 0):
       print("fizz")
      elif (num %5==0):
       print("buzz")
      else:
       print(num)
  
function(choice)
